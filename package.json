{
  "private": true,
  "name": "aesthetic-website",
  "version": "1.0.0",
  "description": "Documentation website for all of Aesthetic.",
  "scripts": {
    "prepare": "beemo create-config --silent",
    "coverage": "yarn run jest --coverage",
    "lint": "beemo eslint",
    "jest": "beemo jest",
    "prettier": "beemo prettier",
    "release": "npx np --yolo --no-yarn",
    "type": "beemo typescript --noEmit",
    "prerelease": "yarn test && yarn run build",
    "pretest": "yarn run type",
    "test": "yarn run jest",
    "posttest": "yarn run eslint",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aesthetic-suite/website.git"
  },
  "license": "MIT",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=10.10.0"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.58",
    "@docusaurus/preset-classic": "^2.0.0-alpha.58",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@milesj/build-tools": "^2.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "beemo": {
    "module": "@milesj/build-tools",
    "drivers": [
      "eslint",
      "prettier",
      "typescript"
    ],
    "settings": {
      "react": true
    }
  }
}
