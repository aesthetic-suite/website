{
  "private": true,
  "name": "aesthetic-website",
  "version": "1.0.0",
  "description": "Documentation website for all of Aesthetic.",
  "scripts": {
    "ds:compile": "aesthetic compile aesthetic ./src/css/system --format web-css",
    "prepare": "beemo create-config --silent",
    "lint": "beemo eslint ./src",
    "format": "beemo prettier",
    "type": "beemo typescript --noEmit",
    "test": "yarn run type && yarn run lint",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aesthetic-suite/website.git"
  },
  "license": "MIT",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=10.10.0"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.69",
    "@docusaurus/preset-classic": "^2.0.0-alpha.69",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@aesthetic/addon-direction": "^0.3.3",
    "@aesthetic/addon-mixins": "^0.2.4",
    "@aesthetic/addon-properties": "^0.2.3",
    "@aesthetic/addon-vendor": "^0.2.3",
    "@aesthetic/cli": "^0.6.0",
    "@aesthetic/compiler": "^0.6.0",
    "@aesthetic/core": "^0.7.0",
    "@aesthetic/react": "^0.6.1",
    "@aesthetic/sss": "^0.6.0",
    "@aesthetic/style": "^0.6.0",
    "@aesthetic/system": "^0.6.0",
    "@aesthetic/types": "^0.4.0",
    "@aesthetic/utils": "^0.6.0",
    "@milesj/build-tools": "^2.14.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "beemo": {
    "module": "@milesj/build-tools",
    "drivers": [
      "eslint",
      "prettier",
      "typescript"
    ],
    "settings": {
      "react": true
    },
    "eslint": {
      "rules": {
        "import/no-unresolved": "off",
        "react/jsx-no-literals": "off",
        "@typescript-eslint/no-unsafe-assignment": "off"
      }
    }
  }
}
